	keywords = {Risk, Context Effect, Correlation, Decision Making, Foreign Countries, Instructional Effectiveness, Knowledge Level, Nuclear Energy, Pretests Posttests, Questionnaires, Regression (Statistics), Role, Safety, Science Education, Scientific Concepts, Student Attitudes},
	pages = {1131--1151},
