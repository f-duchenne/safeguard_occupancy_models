	journal = {Scientific Reports},
	author = {Uhl, Philipp and Franke, Lea A. and Rehberg, Christina and Wollmann, Claudia and Stahlschmidt, Peter and Jeker, Lukas and Brühl, Carsten A.},
	month = sep,
	year = {2016},
	note = {Number: 1
Publisher: Nature Publishing Group},
	pages = {34439},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\XY68SGYX\\Uhl et al. - 2016 - Interspecific sensitivity of bees towards dimethoa.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\V7HVDQVZ\\srep34439.html:text/html},
