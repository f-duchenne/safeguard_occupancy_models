@article{helmus_communities_2010,
	title = {Communities contain closely related species during ecosystem disturbance},
	volume = {13},
	copyright = {© 2009 Blackwell Publishing Ltd/CNRS},
	issn = {1461-0248},
	url = {https://www.onlinelibrary.wiley.com/doi/abs/10.1111/j.1461-0248.2009.01411.x},
	doi = {https://doi.org/10.1111/j.1461-0248.2009.01411.x},
	abstract = {Ecology Letters (2010) 13: 162–174 Abstract Predicting community and species responses to disturbance is complicated by incomplete knowledge about species traits. A phylogenetic framework should partially solve this problem, as trait similarity is generally correlated with species relatedness, closely related species should have similar sensitivities to disturbance. Disturbance should thus result in community assemblages of closely related species. We tested this hypothesis with 18 disturbed and 16 reference whole-lake, long-term zooplankton data sets. Regardless of disturbance type, communities generally contained more closely related species when disturbed. This effect was independent of species richness, evenness, and abundance. Communities already under stress (i.e., those in acidic lakes) changed most when disturbed. Species sensitivities to specific disturbances were phylogenetically conserved, were independent of body size, and could be predicted by the sensitivities of close relatives within the same community. Phylogenetic relatedness can effectively act as a proxy for missing trait information when predicting community and species responses to disturbance.},
	language = {en},
	number = {2},
	urldate = {2021-05-19},
	journal = {Ecology Letters},
	author = {Helmus, Matthew R. and Keller, Wendel (Bill) and Paterson, Michael J. and Yan, Norman D. and Cannon, Charles H. and Rusak, James A.},
	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2009.01411.x},
	keywords = {Biodiversity, phylogenetic signal, zooplankton, environmental filtering, disturbance ecology, extinction prediction, pH, phylogenetic community structure, phylogenetic comparative methods, whole-lake experiment},
	pages = {162--174},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\9IQ73RSF\\Helmus et al. - 2010 - Communities contain closely related species during.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\CUBK6RRL\\j.1461-0248.2009.01411.html:text/html},
