	month = apr,
	year = {2020},
	pmid = {32327596},
	note = {Publisher: American Association for the Advancement of Science
Section: Report},
	pages = {417--420},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\GWBI6LN5\\Klink et al. - 2020 - Meta-analysis reveals declines in terrestrial but .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\45FA8B2W\\417.html:text/html},
