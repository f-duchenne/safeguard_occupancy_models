	publisher = {Oxford University Press},
	author = {Doebeli, Michael},
	editor = {Svensson, Erik and Calsbeek, Ryan},
	month = may,
	year = {2013},
	doi = {10.1093/acprof:oso/9780199595372.003.0014},
	doi = {10.1093/acprof:oso/9780199595372.003.0014},
	pages = {227--242},
	file = {Doebeli - 2013 - Adaptive Dynamics A Framework for Modeling the Lo.pdf:C\:\\Users\\Duchenne\\Zotero\\storage\\YSHQ56T3\\Doebeli - 2013 - Adaptive Dynamics A Framework for Modeling the Lo.pdf:application/pdf},
