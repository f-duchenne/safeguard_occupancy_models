	keywords = {character displacement, coevolution, adaptive dynamics, ecosystem evolution, evolutionary dynamics},
	pages = {465--477},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\PT97UVCY\\Fussmann et al. - 2007 - Eco-evolutionary dynamics of communities and ecosy.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\VC2TPBPW\\j.1365-2435.2007.01275.html:text/html},
