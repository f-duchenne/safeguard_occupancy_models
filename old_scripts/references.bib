	keywords = {food webs, coexistence, structural stability, Białowiez˙a Primeval Forest, ecological networks dynamics, homeostasis, predator–prey systems},
	pages = {865--873},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\JKC7RQR5\\Saavedra et al. - 2016 - Seasonal species interactions minimize the impact .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\FXY6T7WU\\15-1013.html:text/html},
