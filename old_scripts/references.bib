	pages = {3--13},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\H2NSC2FN\\Lurgi et al. - 2016 - The effects of space and diversity of interaction .pdf:application/pdf},
}

@article{pontarp_latitudinal_2019,
	title = {The {Latitudinal} {Diversity} {Gradient}: {Novel} {Understanding} through {Mechanistic} {Eco}-evolutionary {Models}},
