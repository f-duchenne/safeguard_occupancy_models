	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jvs.12683},
	keywords = {habitat fragmentation, climate change, eastern Pyrenees, grazing ecology, mountain grasslands, plant species richness},
	pages = {1029--1039},
