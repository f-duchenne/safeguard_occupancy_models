	keywords = {plant–animal interactions, bird–flower visitation network, flower morphology, size coupling},
	pages = {339--347},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\8ZJTCBMV\\Biddick et Burns - 2018 - Phenotypic trait matching predicts the topology of.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\6RM24EUT\\1749-4877.html:text/html},
