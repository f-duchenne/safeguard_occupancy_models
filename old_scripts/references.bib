	author = {Tikhonov, Gleb and Abrego, Nerea and Dunson, David and Ovaskainen, Otso},
	year = {2017},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/2041-210X.12723},
	keywords = {environmental change, interaction network, co-occurrence, biotic interaction, interaction outcome, latent factor, species distribution model, stress-gradient hypothesis},
	pages = {443--452},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\97UD4VN9\\Tikhonov et al. - 2017 - Using joint species distribution models for evalua.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\XB8HX3RN\\2041-210X.html:text/html},
