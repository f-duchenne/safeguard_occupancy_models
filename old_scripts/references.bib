	urldate = {2018-08-15},
	journal = {Ecology Letters},
	author = {Altermatt, Florian},
	month = dec,
	year = {2010},
	keywords = {Lepidoptera, phenology, climate change, flight period, Butterfly, generation, moth, species traits, voltinism},
