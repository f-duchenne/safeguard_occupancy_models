	keywords = {macroevolution, morphospace, comparative methods, Adaptive radiation, disparity, diversity-dependence, phylogenetics, protracted speciation},
	pages = {2006--2017},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\4T539KB2\\Aristide and Morlon - 2019 - Understanding the effect of competition during evo.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\8ESDPHSJ\\ele.html:text/html},
