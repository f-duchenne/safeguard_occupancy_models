	author = {Bartomeus, Ignasi and Saavedra, Serguei and Rohr, Rudolf P. and Godoy, Oscar},
	month = mar,
	year = {2021},
	pmid = {33727421},
	note = {Publisher: National Academy of Sciences
Section: Biological Sciences},
	keywords = {mutualism, species interactions, competition, persistence, probability},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\NN37F8QF\\Bartomeus et al. - 2021 - Experimental evidence of the importance of multitr.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\GXD5CJV9\\e2023872118.html:text/html},
