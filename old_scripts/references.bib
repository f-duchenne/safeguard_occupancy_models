	keywords = {biodiversity, β-diversity, limiting similarity, environmental distance, meta-communities, meta-ecosystems, meta-food-webs, spatial distance},
	pages = {1225--1236},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\2RGA9MRQ\\Bauer et al. - 2022 - Biotic filtering by species’ interactions constrai.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\NWKIPJH4\\ele.html:text/html},
