	note = {\_eprint: https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1111/j.1755-263X.2009.00049.x},
	keywords = {monitoring, amnesia, bushmeat, change blindness, Equatorial Guinea, farmland birds, Gabon, memory illusion, Perceptions, UK},
