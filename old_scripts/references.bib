	keywords = {Humans, biodiversity, Europe, participation, Learning, Community Participation, Conservation of Natural Resources, Research, Data Collection, Volunteers, aprendizaje transformador, biodiversidad, collaborative assessment, interacciones, participación, política ciencia-sociedad, science-society-policy interactions, transformative learning, valoración colaborativa},
