	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/07-0451.1},
	keywords = {pollination, phenology, mutualistic network, abundance, ecological network, arctic, constraint, linkages between species, preferential attachment},
	pages = {1573--1582},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\V7MC5I9Q\\Olesen et al. - 2008 - Temporal Dynamics in a Pollination Network.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\IAC3TIWS\\07-0451.html:text/html},
