	author = {Graham, Catherine H. and Weinstein, Ben G.},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13084},
	keywords = {interaction turnover, Bayesian, dissimilarity, evolutionary history, interaction beta-diversity, network ecology, prediction, rewiring, scale, species turnover},
	pages = {1299--1310},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\UPEK6CHZ\\Graham and Weinstein - 2018 - Towards a predictive model of species interaction .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\45ZB7ZEI\\ele.html:text/html},
}

@article{temeles_role_2002,
