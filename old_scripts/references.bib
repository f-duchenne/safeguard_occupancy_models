	author = {Whitham, Thomas G. and Bailey, Joseph K. and Schweitzer, Jennifer A. and Shuster, Stephen M. and Bangert, Randy K. and LeRoy, Carri J. and Lonsdorf, Eric V. and Allan, Gery J. and DiFazio, Stephen P. and Potts, Brad M. and Fischer, Dylan G. and Gehring, Catherine A. and Lindroth, Richard L. and Marks, Jane C. and Hart, Stephen C. and Wimp, Gina M. and Wooley, Stuart C.},
	month = jul,
