	note = {Publisher: Proceedings of the National Academy of Sciences},
	pages = {e2211223120},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\TWYSIT9H\\Janousek et al. - 2023 - Recent and future declines of a historically wides.pdf:application/pdf},
