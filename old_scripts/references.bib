	keywords = {pollination, phylogenetic signal, forbidden links, Argentina, Mendoza, Monte Desert, neutrality, phenotypic complementarity, plant–pollinator network, Villavicencio network},
	pages = {2039--2046},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\HDX5K4NQ\\Vázquez et al. - 2009 - Evaluating multiple determinants of the structure .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\436ZCJSP\\08-1837.html:text/html},
