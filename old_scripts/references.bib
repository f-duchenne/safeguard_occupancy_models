	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2009.01437.x},
	keywords = {pollination, extinction, mutualism, complex networks, Behaviour, habitat restoration, Mauritius, network re-wiring},
	pages = {442--452},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\G5YTV63G\\Kaiser-Bunbury et al. - 2010 - The robustness of pollination networks to the loss.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\MME8BHZL\\j.1461-0248.2009.01437.html:text/html},
}

@article{rigal_farmland_2023,
	title = {Farmland practices are driving bird population decline across {Europe}},
	volume = {120},
	url = {https://www.pnas.org/doi/abs/10.1073/pnas.2216573120},
	doi = {10.1073/pnas.2216573120},
	abstract = {Declines in European bird populations are reported for decades but the direct effect of major anthropogenic pressures on such declines remains unquantified. Causal relationships between pressures and bird population responses are difficult to identify as pressures interact at different spatial scales and responses vary among species. Here, we uncover direct relationships between population time-series of 170 common bird species, monitored at more than 20,000 sites in 28 European countries, over 37 y, and four widespread anthropogenic pressures: agricultural intensification, change in forest cover, urbanisation and temperature change over the last decades. We quantify the influence of each pressure on population time-series and its importance relative to other pressures, and we identify traits of most affected species. We find that agricultural intensification, in particular pesticides and fertiliser use, is the main pressure for most bird population declines, especially for invertebrate feeders. Responses to changes in forest cover, urbanisation and temperature are more species-specific. Specifically, forest cover is associated with a positive effect and growing urbanisation with a negative effect on population dynamics, while temperature change has an effect on the dynamics of a large number of bird populations, the magnitude and direction of which depend on species' thermal preferences. Our results not only confirm the pervasive and strong effects of anthropogenic pressures on common breeding birds, but quantify the relative strength of these effects stressing the urgent need for transformative changes in the way of inhabiting the world in European countries, if bird populations shall have a chance of recovering.},
	number = {21},
	urldate = {2023-08-02},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Rigal, Stanislas and Dakos, Vasilis and Alonso, Hany and Auniņš, Ainārs and Benkő, Zoltán and Brotons, Lluís and Chodkiewicz, Tomasz and Chylarecki, Przemysław and de Carli, Elisabetta and del Moral, Juan Carlos and Domşa, Cristian and Escandell, Virginia and Fontaine, Benoît and Foppen, Ruud and Gregory, Richard and Harris, Sarah and Herrando, Sergi and Husby, Magne and Ieronymidou, Christina and Jiguet, Frédéric and Kennedy, John and Klvaňová, Alena and Kmecl, Primož and Kuczyński, Lechosław and Kurlavičius, Petras and Kålås, John Atle and Lehikoinen, Aleksi and Lindström, Åke and Lorrillière, Romain and Moshøj, Charlotte and Nellis, Renno and Noble, David and Eskildsen, Daniel Palm and Paquet, Jean-Yves and Pélissié, Mathieu and Pladevall, Clara and Portolou, Danae and Reif, Jiří and Schmid, Hans and Seaman, Benjamin and Szabo, Zoltán D. and Szép, Tibor and Florenzano, Guido Tellini and Teufelbauer, Norbert and Trautmann, Sven and van Turnhout, Chris and Vermouzek, Zdeněk and Vikstrøm, Thomas and Voříšek, Petr and Weiserbs, Anne and Devictor, Vincent},
	month = may,
	year = {2023},
	note = {Publisher: Proceedings of the National Academy of Sciences},
	pages = {e2216573120},
