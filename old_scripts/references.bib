	author = {Ficetola, Gentile Francesco and Taberlet, Pierre and Coissac, Eric},
	year = {2016},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/1755-0998.12508},
	keywords = {bioinformatics, controls, eDNA, laboratory conditions, occupancy, sampling error},
