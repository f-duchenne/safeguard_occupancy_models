}

@article{goldewijk_hyde_2011,
	title = {The {HYDE} 3.1 spatially explicit database of human-induced global land-use change over the past 12,000 years},
	volume = {20},
	copyright = {© 2010 Blackwell Publishing Ltd},
	issn = {1466-8238},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1466-8238.2010.00587.x},
	doi = {10.1111/j.1466-8238.2010.00587.x},
	abstract = {Aim This paper presents a tool for long-term global change studies; it is an update of the History Database of the Global Environment (HYDE) with estimates of some of the underlying demographic and agricultural driving factors. Methods Historical population, cropland and pasture statistics are combined with satellite information and specific allocation algorithms (which change over time) to create spatially explicit maps, which are fully consistent on a 5′ longitude/latitude grid resolution, and cover the period 10,000 bc to ad 2000. Results Cropland occupied roughly less than 1\% of the global ice-free land area for a long time until ad 1000, similar to the area used for pasture. In the centuries that followed, the share of global cropland increased to 2\% in ad 1700 (c. 3 million km2) and 11\% in ad 2000 (15 million km2), while the share of pasture area grew from 2\% in ad 1700 to 24\% in ad 2000 (34 million km2) These profound land-use changes have had, and will continue to have, quite considerable consequences for global biogeochemical cycles, and subsequently global climate change. Main conclusions Some researchers suggest that humans have shifted from living in the Holocene (emergence of agriculture) into the Anthropocene (humans capable of changing the Earth's atmosphere) since the start of the Industrial Revolution. But in the light of the sheer size and magnitude of some historical land-use changes (e.g. as result of the depopulation of Europe due to the Black Death in the 14th century and the aftermath of the colonization of the Americas in the 16th century) we believe that this point might have occurred earlier in time. While there are still many uncertainties and gaps in our knowledge about the importance of land use (change) in the global biogeochemical cycle, we hope that this database can help global (climate) change modellers to close parts of this gap.},
	language = {en},
	number = {1},
	urldate = {2019-03-19},
	journal = {Global Ecology and Biogeography},
	author = {Goldewijk, Kees Klein and Beusen, Arthur and Drecht, Gerard van and Vos, Martine de},
	year = {2011},
	keywords = {Agricultural development, cropland, DGVM, EMIC, ESM, historical population, human impact, IPCC, land use, pasture},
	pages = {73--86},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\XUYZSC8F\\Goldewijk et al. - 2011 - The HYDE 3.1 spatially explicit database of human-.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\FCCNIXC3\\j.1466-8238.2010.00587.html:text/html},
}

@article{loeuille_eco-evolutionary_2019,
	title = {Eco-evolutionary dynamics in a disturbed world: implications for the maintenance of ecological networks},
	volume = {8},
	issn = {2046-1402},
	shorttitle = {Eco-evolutionary dynamics in a disturbed world},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6347037/},
	doi = {10.12688/f1000research.15629.1},
	abstract = {Past management of exploited species and of conservation issues has often ignored the evolutionary dynamics of species. During the 70s and 80s, evolution was mostly considered a slow process that may be safely ignored for most management issues. However, in recent years, examples of fast evolution have accumulated, suggesting that time scales of evolutionary dynamics (variations in genotype frequencies) and of ecological dynamics (variations in species densities) are often largely comparable, so that complex feedbacks commonly exist between the ecological and the evolutionary context (“eco-evolutionary dynamics”). While a first approach is of course to consider the evolution of a given species, in ecological communities, species are interlinked by interaction networks. In the present article, I discuss how species (co)evolution in such a network context may alter our understanding and predictions for species coexistence, given the disturbed world we live in. I review some concepts and examples suggesting that evolution may enhance the robustness of ecological networks and then show that, in many situations, the reverse may also happen, as evolutionary dynamics can harm diversity maintenance in various ways. I particularly focus on how evolution modifies indirect effects in ecological networks, then move to coevolution and discuss how the outcome of coevolution for species coexistence depends on the type of interaction (mutualistic or antagonistic) that is considered. I also review examples of phenotypes that are known to be important for ecological networks and shown to vary rapidly given global changes. Given all these components, evolution produces indirect eco-evolutionary effects within networks that will ultimately influence the optimal management of the current biodiversity crisis.},
	urldate = {2019-03-19},
	journal = {F1000Research},
	author = {Loeuille, Nicolas},
	month = jan,
	year = {2019},
	pmid = {30728953},
	pmcid = {PMC6347037},
	file = {PubMed Central Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\5DIXRMFQ\\Loeuille - 2019 - Eco-evolutionary dynamics in a disturbed world im.pdf:application/pdf},
}

