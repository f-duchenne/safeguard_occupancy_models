	file = {PubMed Central Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\4J36P9F7\\Vázquez et al. - 2009 - Uniting pattern and process in plant–animal mutual.pdf:application/pdf},
}

@article{edwards_evolutionarily_2018,
	title = {Evolutionarily stable communities: a framework for understanding the role of trait evolution in the maintenance of diversity},
