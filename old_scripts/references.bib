	year = {2009},
	keywords = {food webs, specialization, connectance, indirect effects, inverse Jacobian matrix, linkage density, press perturbation},
	pages = {2426--2433},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\RNQPMYF8\\Montoya et al. - 2009 - Press perturbations and indirect effects in real f.pdf:application/pdf},
}

