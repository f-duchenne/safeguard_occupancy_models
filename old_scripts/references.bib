	keywords = {phylogenetic community structure, phylogenetic structure, evolutionary ecology, codivergence, cophylogeny, cospeciation, covicariance, phylogenetic congruence},
	pages = {932--940},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\EYJQGSA8\\Hutchinson et al. - 2017 - paco implementing Procrustean Approach to Cophylo.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\4UXVPFIA\\2041-210X.html:text/html},
}

