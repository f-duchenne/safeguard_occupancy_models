	keywords = {indirect interactions, food chain, Behaviour, predation risk, trait-mediated, trophic cascade},
	pages = {1245--1252},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\XJ9KPAWH\\Trussell et al. - 2006 - Habitat effects on the relative importance of trai.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\PUATXGQG\\j.1461-0248.2006.00981.html:text/html},
