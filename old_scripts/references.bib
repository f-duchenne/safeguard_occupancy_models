@article{buckland_geometric_2011,
	title = {The geometric mean of relative abundance indices: a biodiversity measure with a difference},
	volume = {2},
	issn = {2150-8925},
	shorttitle = {The geometric mean of relative abundance indices},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1890/ES11-00186.1},
	doi = {10.1890/ES11-00186.1},
	abstract = {The 2010 Biodiversity Target of the Convention on Biological Diversity (CBD), set in 2002, which stated that there should be ‘a significant reduction of the current rate of biodiversity loss' by 2010, highlighted the need for informative and tractable metrics that can be used to evaluate change in biological diversity. While the subsequent Aichi 2020 targets are more wide-ranging, they also seek to reduce the rate of biodiversity loss. The geometric mean of relative abundance indices, G, is increasingly being used to examine trends in biological diversity and to assess whether biodiversity targets are being met. Here, we explore the mathematical and statistical properties of G that make it useful for judging temporal change in biological diversity, and we discuss its advantages and limitations relative to other measures. We demonstrate that the index reflects trends in both abundance and evenness, and that it is not prone to bias when detectability of individuals varies by species. We note that it allows data from different surveys to be combined to generate a composite index. However, the index exhibits high variance and unstable behaviour when rarely-recorded species are included in the analyses.},
	language = {en},
	number = {9},
	urldate = {2022-04-27},
	journal = {Ecosphere},
	author = {Buckland, Stephen T. and Studeny, Angelika C. and Magurran, Anne E. and Illian, Janine B. and Newson, Stuart E.},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/ES11-00186.1},
	keywords = {relative abundance, 2010 Biodiversity Target, 2020 Aichi Biodiversity Targets, biodiversity measure, evenness, geometric mean},
	pages = {art100},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\M6JL89P7\\Buckland et al. - 2011 - The geometric mean of relative abundance indices .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\AI3XHTFI\\ES11-00186.html:text/html},
