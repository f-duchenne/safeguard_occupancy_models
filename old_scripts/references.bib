	year = {2000},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1461-0248.2000.00161.x},
	keywords = {evolution, community structure, host range, resistance, Bacteria, bacteriophage, mutations},
	pages = {362--377},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\MUP7YCIK\\Bohannan and Lenski - 2000 - Linking genetic change to community evolution ins.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\B74LGDJQ\\j.1461-0248.2000.00161.html:text/html},
