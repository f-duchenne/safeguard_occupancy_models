	pages = {2091--2106},
	file = {Full Text:C\:\\Users\\Duchenne\\Zotero\\storage\\3FXN8FKH\\Yamamichi et al. - 2022 - Integrating eco-evolutionary dynamics and modern c.pdf:application/pdf},
