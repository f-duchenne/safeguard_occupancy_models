	month = feb,
	year = {2021},
	pmid = {33526655},
	note = {Publisher: National Academy of Sciences
Section: Biological Sciences},
	keywords = {ecological networks, community ecology, biodiversity change, mountain ecosystems, plant interaction networks},
	file = {Full Text:C\:\\Users\\Duchenne\\Zotero\\storage\\XZAYCUWE\\Losapio et al. - 2021 - Network motifs involving both competition and faci.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\S2SXRLY6\\e2005759118.html:text/html},
