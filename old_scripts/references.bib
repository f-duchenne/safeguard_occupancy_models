@article{kallimanis_plantpollinator_2009,
	title = {Do plant–pollinator interaction networks result from stochastic processes?},
	volume = {220},
	issn = {0304-3800},
	url = {http://www.sciencedirect.com/science/article/pii/S0304380008005474},
	doi = {10.1016/j.ecolmodel.2008.11.008},
	abstract = {Plant–pollinator interaction networks are characterized by several features that cannot be obtained from a totally random network (e.g. nestedness, power law distribution of degree specialization, temporal turnover). One reason is that both plants and pollinators are active for only a part of the year, and so a plant species flowering in spring cannot interact with a pollinator species that is active only in autumn. In this paper we build a stochastic model to simulate the plant–pollinator interaction network, taking into account the duration of activity of each species. To build the model we used an empirical plant–pollinator network from a Mediterranean scrub community surveyed over four years. In our simulated annual cycle we know which plant and pollinator species are active, and thus available to interact. We can obtain simulated plant–pollinator interaction networks with properties similar to the real ones in two different ways: (i) by assuming that the frequency distribution of both plant and pollinator duration of activity follow an exponential function, and that interaction among temporally coexisting species are totally random, and (ii) by assuming more realistic frequency distributions (exponential for pollinators, lognormal for plants) and that the interaction among coexisting species is occurring on a per capita basis. In the latter case we assume that there is a positive relationship between abundance and duration of activity. In our model the starting date of the species activity had little influence on the network structure. We conclude that the observed plant–pollinator network properties can be produced stochastically, and the mechanism shaping the network is not necessarily related to size constraints. Under such conditions co-evolutionary explanations should be given with caution.},
	language = {en},
	number = {5},
	urldate = {2020-09-11},
	journal = {Ecological Modelling},
	author = {Kallimanis, A. S. and Petanidou, T. and Tzanopoulos, J. and Pantis, J. D. and Sgardelis, S. P.},
	month = mar,
	year = {2009},
	keywords = {Network analysis, Phenology, Phenophase},
	pages = {684--693},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\R6SNR63R\\Kallimanis et al. - 2009 - Do plant–pollinator interaction networks result fr.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\A9RXGSA4\\S0304380008005474.html:text/html},
}

@article{levine_beyond_2017,
	title = {Beyond pairwise mechanisms of species coexistence in complex communities},
