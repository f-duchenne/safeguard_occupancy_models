	keywords = {Plant-insect interactions, Species diversity, Plants, Species interactions, Caterpillars, Interaction networks, Trophic interactions, Plant-herbivore interactions},
	pages = {e0203164},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\6DJQICDP\\Lepesqueur et al. - 2018 - Changing interactions among persistent species as .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\PPCTYGR3\\article.html:text/html},
