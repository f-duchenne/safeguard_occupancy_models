	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2009.01437.x},
	keywords = {pollination, extinction, mutualism, complex networks, Behaviour, habitat restoration, Mauritius, network re-wiring},
	pages = {442--452},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\H62J3YVE\\Kaiser-Bunbury et al. - 2010 - The robustness of pollination networks to the loss.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\RPPA46SC\\j.1461-0248.2009.01437.html:text/html},
