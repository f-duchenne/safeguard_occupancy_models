	journal = {Ecology},
	author = {Durka, Walter and Michalski, Stefan G.},
	year = {2012},
	keywords = {macroecology, phylogeny, BIOLFLOR, British flora, German flora, Netherlands flora, PLANTATT, supertree, Swiss flora, vascular plants},
	pages = {2297--2297},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\MNK9VNXH\\Durka et Michalski - 2012 - Daphne a dated phylogeny of a large European flor.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\SLYU2HX8\\12-0743.html:text/html},
