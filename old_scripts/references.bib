@article{baselga_partitioning_2017,
	title = {Partitioning abundance-based multiple-site dissimilarity into components: balanced variation in abundance and abundance gradients},
	volume = {8},
	issn = {2041-210X},
	shorttitle = {Partitioning abundance-based multiple-site dissimilarity into components},
