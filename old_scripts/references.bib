	keywords = {Hymenoptera, Lepidoptera, DNA barcode, trophic interaction},
	pages = {1885--1890},
	file = {Full Text:C\:\\Users\\Duchenne\\Zotero\\storage\\87R4V2IZ\\Wirta et al. - 2014 - Complementary molecular information changes our pe.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\IRZIIGXX\\1885.html:text/html},
