	pages = {20121931},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\5CH9XY63\\Dornelas Maria et al. - 2013 - Quantifying temporal change in biodiversity chall.pdf:application/pdf},
