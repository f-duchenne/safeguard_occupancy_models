	pages = {3843--3852},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\WMRS63GL\\Anderson et al. - 2012 - Phenotypic plasticity and adaptive evolution contr.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\68F6PET4\\rspb.2012.html:text/html},
