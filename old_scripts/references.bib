	journal = {Evolution},
	author = {Sletvold, Nina and Trunschke, Judith and Smit, Mart and Verbeek, Jeffrey and Ågren, Jon},
	year = {2016},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/evo.12881},
	keywords = {natural selection, facilitation, Anacamptis morio, floral evolution, flower color, plant-animal interactions, pollination by deceit},
	pages = {716--724},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\J76TB8ZV\\Sletvold et al. - 2016 - Strong pollinator-mediated selection for increased.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\FFXCNH5N\\evo.html:text/html},
