	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/2041-210X.13796},
	keywords = {food webs, trophic interactions, network ecology, DNA barcoding, metagenomics, multilayer networks, quantification},
	pages = {545--559},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\BKTDHXIZ\\Cuff et al. - 2022 - Overcoming the pitfalls of merging dietary metabar.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\6654TC5B\\2041-210X.html:text/html},
