	keywords = {Network analysis, Nitrogen, Ecosystem function, Carbon cycling and storage, Nutrient cycling, Phosphorus, Soil biodiversity},
	pages = {112--124},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\K8YBSZQS\\Creamer et al. - 2016 - Ecological network analysis reveals the inter-conn.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\MCW3E2TU\\S0929139315300597.html:text/html},
}

@article{coetzee_land-use_2016,
