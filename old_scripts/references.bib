	url = {http://www.sciencedirect.com/science/article/pii/002219109390034O},
	doi = {10.1016/0022-1910(93)90034-O},
	abstract = {Rapid evolution of behavioural and morphological adaptations were found in populations of Drosophila melanogaster exposed to laboratory natural selection in high temperatures. After 10 generations at 25, 27 and 30°C, flies were acclimated to 25°C and assayed for food-searching behaviour and preferred environmental temperature along a gradient. Flies showed significant differences in both traits. After 15 generations at the respective temperatures, flies from the 27 and 30°C populations were significantly smaller than those from the 25°C populations. These three traits were all directly correlated with the temperature treatments. As treatment temperatures increase, flies have more intense search behaviour, prefer higher temperatures and are smaller. The rearing temperatures of flies from the 25 and 30°C populations were reversed in the 12th generation, thereby changing the developmental temperature. This revealed a significant environmental effect for both behaviours. However, both behaviours were also shown to have genetic components, indicating that differences were created by the laboratory natural selection through evolutionary adaptations to temperature. Most previous studies demonstrated adaptations only after many more generations of laboratory natural selection. Additionally, development was retarded at 33°C and males were sterile at 31°C for this strain.},
	language = {en},
	number = {7},
	urldate = {2020-08-12},
	journal = {Journal of Insect Physiology},
	author = {Good, Darrin S.},
	month = jul,
	year = {1993},
	keywords = {Diptera, Genetics, Laboratory natural selection, Searching behaviour, Temperature preference},
	pages = {537--544},
	file = {ScienceDirect Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\HE2UWGSQ\\002219109390034O.html:text/html},
}

