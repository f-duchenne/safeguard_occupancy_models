	keywords = {climate change, adaptive dynamics, biodiversity conservation, body mass evolution, consumer–resource model, eco-evolutionary tipping point, evolutionary rescue, trophic networks},
	pages = {710--722},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\A4WZBG8D\\Yacine et al. - 2021 - Collapse and rescue of evolutionary food webs unde.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\XR9ZV728\\1365-2656.html:text/html},
