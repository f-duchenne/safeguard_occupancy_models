	author = {Stanton, R. L. and Morrissey, C. A. and Clark, R. G.},
	month = feb,
	year = {2018},
	keywords = {Agricultural intensification, Aerial insectivores, Avian declines, Farmland birds, Grassland birds, Habitat loss, Pesticides},
	pages = {244--254},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\ZQKXMPR8\\Stanton et al. - 2018 - Analysis of trends and agricultural drivers of far.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\UE7G27EG\\S016788091730525X.html:text/html},
}

