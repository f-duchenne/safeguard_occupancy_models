	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.12912},
	keywords = {interspecific competition, stability, community assembly, Coexistence, network structure},
	pages = {324--334},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\VE4R6V67\\Maynard et al. - 2018 - Network spandrels reflect ecological assembly.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\HTE74L73\\ele.html:text/html},
