	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1420-9101.2007.01300.x},
	doi = {10.1111/j.1420-9101.2007.01300.x},
	abstract = {The ability of individual organisms to alter morphological and life-history traits in response to the conditions they experience is an example of phenotypic plasticity which is fundamental to any population's ability to deal with short-term environmental change. We currently know little about the prevalence, and evolutionary and ecological causes and consequences of variation in life history plasticity in the wild. Here we outline an analytical framework, utilizing the reaction norm concept and random regression statistical models, to assess the between-individual variation in life history plasticity that may underlie population level responses to the environment at both phenotypic and genetic levels. We discuss applications of this framework to date in wild vertebrate populations, and illustrate how natural selection and ecological constraint may alter a population's response to the environment through their effects at the individual level. Finally, we present future directions and challenges for research into individual plasticity.},
	language = {en},
	number = {3},
	urldate = {2020-01-30},
	journal = {Journal of Evolutionary Biology},
	author = {Nussey, D. H. and Wilson, A. J. and Brommer, J. E.},
	year = {2007},
	keywords = {phenotypic plasticity, reaction norm, environmental change, labile trait, mixed model, random regression, vertebrate, wild population},
	pages = {831--844},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\78JUDGDN\\Nussey et al. - 2007 - The evolutionary ecology of individual phenotypic .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\QYGFW2DP\\j.1420-9101.2007.01300.html:text/html},
}

@article{charmantier_climate_2014,
	title = {Climate change and timing of avian breeding and migration: evolutionary versus plastic changes},
	volume = {7},
	copyright = {© 2013 The Authors. Evolutionary Applications published by John Wiley \& Sons Ltd.},
	issn = {1752-4571},
	shorttitle = {Climate change and timing of avian breeding and migration},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/eva.12126},
	doi = {10.1111/eva.12126},
	abstract = {There are multiple observations around the globe showing that in many avian species, both the timing of migration and breeding have advanced, due to warmer springs. Here, we review the literature to disentangle the actions of evolutionary changes in response to selection induced by climate change versus changes due to individual plasticity, that is, the capacity of an individual to adjust its phenology to environmental variables. Within the abundant literature on climate change effects on bird phenology, only a small fraction of studies are based on individual data, yet individual data are required to quantify the relative importance of plastic versus evolutionary responses. While plasticity seems common and often adaptive, no study so far has provided direct evidence for an evolutionary response of bird phenology to current climate change. This assessment leads us to notice the alarming lack of tests for microevolutionary changes in bird phenology in response to climate change, in contrast with the abundant claims on this issue. In short, at present we cannot draw reliable conclusions on the processes underlying the observed patterns of advanced phenology in birds. Rapid improvements in techniques for gathering and analysing individual data offer exciting possibilities that should encourage research activity to fill this knowledge gap.},
	language = {en},
	number = {1},
	urldate = {2020-01-30},
	journal = {Evolutionary Applications},
	author = {Charmantier, Anne and Gienapp, Phillip},
	year = {2014},
	keywords = {phenology, climate change, phenotypic plasticity, evolution, bird, selection, timing of breeding, timing of migration},
	pages = {15--28},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\6NT4R22T\\Charmantier et Gienapp - 2014 - Climate change and timing of avian breeding and mi.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\ZLA5HNCU\\eva.html:text/html},
}

@article{charmantier_adaptive_2008,
	title = {Adaptive {Phenotypic} {Plasticity} in {Response} to {Climate} {Change} in a {Wild} {Bird} {Population}},
