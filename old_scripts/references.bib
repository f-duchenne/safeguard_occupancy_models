	keywords = {Applied mathematics, Network topology},
	pages = {5470},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\P6WUX26I\\Cai et al. - 2020 - Mutualistic networks emerging from adaptive niche-.pdf:application/pdf},
}

@article{gilarranz_architecture_2012,
	title = {The architecture of weighted mutualistic networks},
	volume = {121},
