	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13582},
	keywords = {structural stability, probability, ecological communities, ensemble theory, experimental tests, nonlinear population dynamics, out-of-sample predictions, statistical mechanics},
	pages = {1511--1521},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\YH8P5ATF\\Saavedra et al. - 2020 - Structural forecasting of species persistence unde.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\RNKCLM5A\\ele.html:text/html},
