@article{vazquez_degree_2005,
	title = {Degree distribution in plant–animal mutualistic networks: forbidden links or random interactions?},
	volume = {108},
	issn = {1600-0706},
	shorttitle = {Degree distribution in plant–animal mutualistic networks},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.0030-1299.2005.13619.x},
	doi = {10.1111/j.0030-1299.2005.13619.x},
	abstract = {Recent studies show that ecological interaction networks depart from the “scale-free” topologies observed in many other real world networks. Such a departure has been hypothesized to result from non-matching biological attributes of species, such as phenology or morphology, that prevent the occurrence of certain interactions (“forbidden links”). Here I compare the topology of 17 plant–animal mutualistic networks with that predicted by a simple null model that assumes that a species’ degree (number of interspecific interactions) is a function of its frequency of interaction. The topology predicted by this null model is strikingly close to that observed in the real networks. Thus, this null model provides a simple alternative interpretation of patterns observed in ecological interaction networks that does not require the existence of non-matching species traits.},
	language = {en},
	number = {2},
	urldate = {2023-04-04},
	journal = {Oikos},
	author = {Vázquez, Diego P.},
	year = {2005},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.0030-1299.2005.13619.x},
	pages = {421--426},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\78SWJIWA\\Vázquez - 2005 - Degree distribution in plant–animal mutualistic ne.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\8QGSQGHW\\j.0030-1299.2005.13619.html:text/html},
}

@article{gonzalez-varo_labile_2016-1,
	title = {The {Labile} {Limits} of {Forbidden} {Interactions}},
	volume = {31},
	issn = {0169-5347},
	url = {https://www.sciencedirect.com/science/article/pii/S0169534716300787},
	doi = {10.1016/j.tree.2016.06.009},
	abstract = {Forbidden links are defined as pairwise interactions that are prevented by the biological traits of the species. We focus here on the neglected importance of intraspecific trait variation in the forbidden link concept. We show how intraspecific trait variability at different spatiotemporal scales, and through ontogeny, reduces the expected prevalence of forbidden interactions. We also highlight how behavior can foster interactions that, from traits, would be predicted to be forbidden. We therefore discuss the drawbacks of frameworks recently developed to infer biotic interactions using available trait data (mean values). Mispredictions can have disproportionate effects on inferences about community dynamics. Thus, we suggest including intraspecific variability in trait-based models and using them to guide the sampling of real interactions in the field for validation.},
	language = {en},
	number = {9},
	urldate = {2023-04-04},
	journal = {Trends in Ecology \& Evolution},
	author = {González-Varo, Juan P. and Traveset, Anna},
	month = sep,
	year = {2016},
	keywords = {ecological networks, biotic interactions, trait matching, intraspecific variability},
	pages = {700--710},
