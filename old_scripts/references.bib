@article{jiguet_population_2010-1,
	title = {Population trends of {European} common birds are predicted by characteristics of their climatic niche},
	volume = {16},
	copyright = {© 2009 Blackwell Publishing Ltd},
	issn = {1365-2486},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-2486.2009.01963.x},
	doi = {10.1111/j.1365-2486.2009.01963.x},
	abstract = {Temperate species are projected to experience the greatest temperature increases across a range of modelled climate change scenarios, and climate warming has been linked to geographical range and population changes of individual species at such latitudes. However, beyond the multiple modelling approaches, we lack empirical evidence of contemporary climate change impacts on populations in broad taxonomic groups and at continental scales. Identifying reliable predictors of species resilience or susceptibility to climate warming is of critical importance in assessing potential risks to species, ecosystems and ecosystem services. Here we analysed long-term trends of 110 common breeding birds across Europe (20 countries), to identify climate niche characteristics, adjusted to other environmental and life history traits, that predict large-scale population changes accounting for phylogenetic relatedness among species. Beyond the now well-documented decline of farmland specialists, we found that species with the lowest thermal maxima (as the mean spring and summer temperature of the hottest part of the breeding distribution in Europe) showed the sharpest declines between 1980 and 2005. Thermal maximum predicted the recent trends independently of other potential predictors. This study emphasizes the need to account for both land-use and climate changes to assess the fate of species. Moreover, we highlight that thermal maximum appears as a reliable and simple predictor of the long-term trends of such endothermic species facing climate change.},
	language = {en},
	number = {2},
	urldate = {2020-08-08},
	journal = {Global Change Biology},
	author = {Jiguet, Frédéric and Gregory, Richard D. and Devictor, Vincent and Green, Rhys E. and Voříšek, Petr and Strien, Arco Van and Couvet, Denis},
	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1365-2486.2009.01963.x},
	keywords = {global warming, breeding birds, climate niche, climate warming, long-term trends},
	pages = {497--505},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\F6MGTK8N\\Jiguet et al. - 2010 - Population trends of European common birds are pre.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\XVR4W5AV\\j.1365-2486.2009.01963.html:text/html},
