Section: Biological Sciences},
	keywords = {pollinator decline, mutualism, ecological networks, resilience, critical transition},
	pages = {17546--17551},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\TT5MG2XP\\Dakos et Bascompte - 2014 - Critical slowing down as early warning for the ons.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\2BRIRPVK\\17546.html:text/html},
}

@article{lever_sudden_2014,
