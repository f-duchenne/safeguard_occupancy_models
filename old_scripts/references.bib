	note = {Publisher: Royal Society},
	keywords = {macroecology, habitat specialization, breeding bird survey, community specialization index},
	pages = {780--782},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\E9QJNJTK\\Le Viol et al. - 2012 - More and more generalists two decades of changes .pdf:application/pdf},
