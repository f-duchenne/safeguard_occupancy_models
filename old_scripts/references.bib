	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\BNAFPN7C\\Ficetola et al. - 2016 - How to limit false positives in environmental DNA .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\PZLXIDTX\\1755-0998.html:text/html},
