	keywords = {community stability, Adaptive dynamics, directional selection, diversity-stability debate, population dynamics stability},
	pages = {1536--1545},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\7DNHB8GT\\Loeuille - 2010 - Influence of evolution on the stability of ecologi.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\FWY57IX3\\j.1461-0248.2010.01545.html:text/html},
}

