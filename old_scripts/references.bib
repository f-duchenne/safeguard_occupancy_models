	abstract = {Genetic assimilation emerges from selection on phenotypic plasticity. Yet, commonly used quantitative genetics models of linear reaction norms considering intercept and slope as traits do not mimic the full process of genetic assimilation. We argue that intercept–slope reaction norm models are insufﬁcient representations of genetic effects on linear reaction norms and that considering reaction norm intercept as a trait is unfortunate because the deﬁnition of this trait relates to a speciﬁc environmental value (zero) and confounds genetic effects on reaction norm elevation with genetic effects on environmental perception. Instead, we suggest a model with three traits representing genetic effects that, respectively, (i) are independent of the environment, (ii) alter the sensitivity of the phenotype to the environment and (iii) determine how the organism perceives the environment. The model predicts that, given sufﬁcient additive genetic variation in environmental perception, the environmental value at which reaction norms tend to cross will respond rapidly to selection after an abrupt environmental change, and eventually becomes equal to the new mean environment. This readjustment of the zone of canalization becomes completed without changes in genetic correlations, genetic drift or imposing any ﬁtness costs of maintaining plasticity. The asymptotic evolutionary outcome of this threetrait linear reaction norm generally entails a lower degree of phenotypic plasticity than the two-trait model, and maximum expected ﬁtness does not occur at the mean trait values in the population.},
	language = {en},
	number = {3},
	urldate = {2020-04-04},
	journal = {Journal of Evolutionary Biology},
	author = {Ergon, T. and Ergon, R.},
	month = mar,
	year = {2017},
	pages = {486--500},
	file = {Ergon et Ergon - 2017 - When three traits make a line evolution of phenot.pdf:C\:\\Users\\Duchenne\\Zotero\\storage\\R77LUKRB\\Ergon et Ergon - 2017 - When three traits make a line evolution of phenot.pdf:application/pdf},
}

@article{redhead_potential_2018,
	title = {Potential landscape-scale pollinator networks across {Great} {Britain}: structure, stability and influence of agricultural land cover},
	volume = {21},
	copyright = {© 2018 The Authors Ecology Letters published by CNRS and John Wiley \& Sons Ltd},
	issn = {1461-0248},
	shorttitle = {Potential landscape-scale pollinator networks across {Great} {Britain}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.13157},
	doi = {10.1111/ele.13157},
	abstract = {Understanding spatial variation in the structure and stability of plant–pollinator networks, and their relationship with anthropogenic drivers, is key for maintaining pollination services and mitigating declines. Constructing sufficient networks to examine patterns over large spatial scales remains challenging. Using biological records (citizen science), we constructed potential plant–pollinator networks at 10 km resolution across Great Britain, comprising all potential interactions inferred from recorded floral visitation and species co-occurrence. We calculated network metrics (species richness, connectance, pollinator and plant generality) and adapted existing methods to assess robustness to sequences of simulated plant extinctions across multiple networks. We found positive relationships between agricultural land cover and both pollinator generality and robustness to extinctions under several extinction scenarios. Increased robustness was attributable to changes in plant community composition (fewer extinction-prone species) and network structure (increased pollinator generality). Thus, traits enabling persistence in highly agricultural landscapes can confer robustness to potential future perturbations on plant–pollinator networks.},
	language = {en},
	number = {12},
	urldate = {2020-04-03},
	journal = {Ecology Letters},
	author = {Redhead, John W. and Woodcock, Ben A. and Pocock, Michael J. O. and Pywell, Richard F. and Vanbergen, Adam J. and Oliver, Tom H.},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13157},
	keywords = {Citizen science, pollination, food webs, ecological networks, ecosystem services, stability, resilience},
	pages = {1821--1832},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\57K6ENJM\\Redhead et al. - 2018 - Potential landscape-scale pollinator networks acro.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\Q9A327BG\\ele.html:text/html},
