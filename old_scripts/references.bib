@article{hedtke_bee_2013,
	title = {The bee tree of life: a supermatrix approach to apoid phylogeny and biogeography},
	volume = {13},
	issn = {1471-2148},
	shorttitle = {The bee tree of life},
	url = {https://doi.org/10.1186/1471-2148-13-138},
