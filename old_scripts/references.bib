	pages = {12643},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\IJ7K6RKS\\Bertrand et al. - 2016 - Ecological constraints increase the climatic debt .pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\HE5NEJ5Z\\ncomms12643.html:text/html},
}

