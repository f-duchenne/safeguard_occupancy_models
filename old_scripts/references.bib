	keywords = {community dynamics, urbanization, breeding bird survey, community homogenization, dinámica de la comunidad, especialización de especies, habitat disturbance, homogeneización de la comunidad, monitoreo de aves residentes, perturbación del hábitat, probability of detection, species specialization, urbanización},
	pages = {741--751},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\YCUQ7QUZ\\Devictor et al. - 2007 - Functional Homogenization Effect of Urbanization o.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\6H6VNA5X\\j.1523-1739.2007.00671.html:text/html},
