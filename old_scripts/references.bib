	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.12957},
	keywords = {preferential attachment, succession, Community assembly, ecosystem development, mutualist network, mycorrhizal symbiosis, retrogression},
	pages = {896--904},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\VQFF4Z4S\\Tylianakis et al. - 2018 - Symmetric assembly and disassembly processes in an.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\ULHSTKII\\ele.html:text/html},
}

@article{ponisio_opportunistic_2017,
