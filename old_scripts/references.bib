	journal = {Biotropica},
	author = {McDade, Lucinda A. and Weeks, Jennifer A.},
	year = {2004},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1744-7429.2004.tb00313.x},
	keywords = {nectar, Costa Rica, Acanthaceae, bummingbird pollination, Malvaceae, nectar robbing Panama, Rubiaceae, standing crop},
	pages = {216--230},
