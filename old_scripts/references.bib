	pmid = {15615687},
	pages = {2605--2611},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\C983BTUW\\Memmott et al. - 2004 - Tolerance of pollination networks to species extin.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\ZSMSZUYZ\\2605.html:text/html},
}

@article{lefebvre_altitudinal_2018,
