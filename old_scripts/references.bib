	year = {2017},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1002/ecm.1263},
	keywords = {community dynamics, feasibility, invasion criterion, multiple competitors, niche and fitness differences, pairwise effects, structural stability},
	pages = {470--486},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\RKXWTNCB\\Saavedra et al. - 2017 - A structural approach for understanding multispeci.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\RRSDL7NP\\ecm.html:text/html},
