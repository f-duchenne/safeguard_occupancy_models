	pages = {1250--1257},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\9NUI7XQ3\\Richman et al. - 2017 - Foraging strategy predicts foraging economy in a f.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\QG7S26AE\\oik.html:text/html},
