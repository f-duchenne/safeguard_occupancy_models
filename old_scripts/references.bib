	file = {PubMed Central Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\HAWM3SXP\\Peralta‐Maraver et al. - 2017 - Winter is coming Food web structure and seasonali.pdf:application/pdf},
