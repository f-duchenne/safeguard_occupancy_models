	month = oct,
	year = {2017},
	note = {Publisher: Public Library of Science},
	keywords = {Insects, Species diversity, Biomass (ecology), Conservation science, Grasslands, Herbs, Land use, Seasons},
	pages = {e0185809},
	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\W8DK9CKK\\Hallmann et al. - 2017 - More than 75 percent decline over 27 years in tota.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\SFFZXLPG\\journal.pone.html:text/html},
