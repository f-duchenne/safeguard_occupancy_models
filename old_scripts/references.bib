	file = {Full Text PDF:C\:\\Users\\Duchenne\\Zotero\\storage\\PV9QQU5H\\Shirey et al. - 2023 - Occupancy–detection models with museum specimen da.pdf:application/pdf;Snapshot:C\:\\Users\\Duchenne\\Zotero\\storage\\LQ9A8BQM\\2041-210X.html:text/html},
